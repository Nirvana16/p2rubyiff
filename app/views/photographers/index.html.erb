<p id="notice"><%= notice %></p>

<h1>Photographers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Email</th>
      <th>Password</th>
      <th>String</th>
      <th>Avatar</th>
      <th>Portfolio link</th>
      <th>Bio</th>
      <th>Display name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photographers.each do |photographer| %>
      <tr>
        <td><%= photographer.name %></td>
        <td><%= photographer.phone %></td>
        <td><%= photographer.city %></td>
        <td><%= photographer.state %></td>
        <td><%= photographer.country %></td>
        <td><%= photographer.email %></td>
        <td><%= photographer.password %></td>
        <td><%= photographer.string %></td>
        <td><%= photographer.avatar %></td>
        <td><%= photographer.portfolio_link %></td>
        <td><%= photographer.bio %></td>
        <td><%= photographer.display_name %></td>
        <td><%= link_to 'Show', photographer %></td>
        <td><%= link_to 'Edit', edit_photographer_path(photographer) %></td>
        <td><%= link_to 'Destroy', photographer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photographer', new_photographer_path %>
